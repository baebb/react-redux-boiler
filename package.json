{
  "name": "react-redux-boiler",
  "version": "0.2.0",
  "description": "Simple React Redux app boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -d --progress --inline --port 9999",
    "build:production": "rm -rf ./build && NODE_ENV=\"production\" webpack --progress --colors",
    "build:serve": "http-server ./build -p 8079",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "format": "eslint --fix src/ test/ --ext js"
  },
  "author": "Ross Dyson iam@rossdyson.com",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "jsdom": "^10.1.0",
    "mocha": "^3.4.1",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap-sass": "^3.3.7",
    "history": "^4.6.1",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-little-router": "^13.1.2",
    "redux-thunk": "^2.2.0"
  }
}
